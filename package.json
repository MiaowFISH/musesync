{
  "name": "@musesync/workspace",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "app",
    "backend",
    "shared"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:app": "turbo run dev --filter=app",
    "dev:backend": "turbo run dev --filter=backend",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "turbo": "^2.8.7",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "react-native-track-player": "patch:react-native-track-player@npm%3A4.1.2#.yarn/patches/react-native-track-player-npm-4.1.2-b3ddb100d7.patch"
  },
  "packageManager": "yarn@4.12.0"
}
