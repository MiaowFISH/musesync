{
  "name": "@musesync/workspace",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "app",
    "backend",
    "shared"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:app": "turbo run dev --filter=app",
    "dev:backend": "turbo run dev --filter=backend",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "turbo": "^2.8.7",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@expo/cli@npm:54.0.20": "patch:@expo/cli@npm%3A54.0.20#~/.yarn/patches/@expo-cli-npm-54.0.20-5db1021132.patch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": "pnpm run lint"
  },
  "packageManager": "yarn@4.12.0"
}
